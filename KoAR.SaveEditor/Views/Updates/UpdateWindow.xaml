<Window x:Class="KoAR.SaveEditor.Views.Updates.UpdateWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:local="clr-namespace:KoAR.SaveEditor.Views.Updates"
    mc:Ignorable="d" d:DataContext="{d:DesignInstance {x:Type local:UpdateViewModel}}"
    Icon="/app.ico" Title="Update Available - KoAR Save Editor">
    <Window.Resources>
        <ObjectDataProvider x:Key="ViewModel" ObjectType="{x:Type local:UpdateViewModel}" />
    </Window.Resources>
    <Window.DataContext>
        <Binding Source="{StaticResource ViewModel}" />
    </Window.DataContext>
    <Grid>
        
    </Grid>
</Window>
